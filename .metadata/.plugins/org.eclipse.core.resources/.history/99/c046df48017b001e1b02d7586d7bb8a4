package _13_1_ArrayOfObject;

class students{
	int rollno;
	String name;
	int marks;
}

public class ArrayOfObject {
	public static void main(String []args)
	{
		students S1 = new students();
		S1.rollno = 1;
		S1.name = "Yugandhar";
		S1.marks = 96;
		
		students S2 = new students();
		S2.rollno = 2;
		S2.name = "King";
		S2.marks = 99;
		
//		students S3 = new students();
		S3.rollno = 3;
		S3.name = "Kong";
		S3.marks = 89;
		
	}

}
